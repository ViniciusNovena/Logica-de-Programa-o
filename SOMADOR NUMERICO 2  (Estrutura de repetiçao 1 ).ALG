algoritmo "Somador Numerico 2 "
var
Cont , S , N: Inteiro
inicio
      Cont <- 1
      S <- 0
Enquanto (Cont <= 5) Faca
         Escreva (" Digite o ",cont, " o. valor: ")
         Leia (N)
         S <- S + N
         Cont <- Cont + 1
FimEnquanto
Escreval("A soma de todos os valor foi ", S)
fimalgoritmo