algoritmo "semnome"
var
N1 , N2 , SomaN , Cont: Inteiro
inicio
N1 <- 0
N2 <- 1
SomaN <- 0
Escreval ("Digite o 1º valor")
Leia (N1)
Escreval ("Digite o 2º Valor")
Leia(N2)
        Para Cont <-1 até 10 faca
        SomaN <- N1 + N2
        Escreva(SomaN)
        N1 <- N2
        N2 <- SomaN
        Fimpara

fimalgoritmo