algoritmo "IDADE CREUZA "
var
   ANO_ATUAL , ANO_NASC , IDADE: INTEIRO
inicio
      ESCREVA (" EM QUE ANO ESTAMOS?")
      LEIA(ANO_ATUAL)
      ESCREVA("EM QUE ANO VOCÊ NASCEU?")
      LEIA(ANO_NASC)
      IDADE <- (ANO_ATUAL - ANO_NASC)
      ESCREVA (" A SUA IDADE SERA:" , IDADE)

fimalgoritmo